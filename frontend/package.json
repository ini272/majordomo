{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "bun": ">=1.0.0",
    "npm": "please-use-bun",
    "yarn": "please-use-bun",
    "pnpm": "please-use-bun"
  },
  "scripts": {
    "dev": "vite",
    "dev:cloudflare": "bun run scripts/cloudflareDev.ts",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test:cloudflare": "bun test scripts/cloudflareTunnel.test.ts",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css}\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fontsource/cinzel": "^5.2.8",
    "@fontsource/im-fell-english": "^5.2.6",
    "beautiful-mermaid": "^0.1.3",
    "framer-motion": "^12.34.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.1.4",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
